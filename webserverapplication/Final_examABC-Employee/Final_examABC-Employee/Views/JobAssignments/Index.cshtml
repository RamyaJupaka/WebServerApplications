@model IEnumerable<Final_examABC_Employee.Models.JobAssignments>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AssignemtDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobCodeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AssignemtDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdNavigation.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobCodeNavigation.JobCode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.JobAssignmentsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.JobAssignmentsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.JobAssignmentsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
